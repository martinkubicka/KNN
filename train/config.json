{
    "data_dir": "data/database",
    "indicies": {
        "train": "data/train_ids.csv",
        "test": "data/test_ids.csv"
    },

    "batch_size": 32,
    "scheduler": {
        "step_size": 10,
        "gamma": 0.1
    },
    "momentum": 0.9,

    "embedding": {
        "dim": 256,
        "dropout": 0.5
    },

    "architecture":{
        "name": "vit",
        "patch_size": 16,
        "num_classes": 4092  
    },

    "loss": {
        "name": "adaface"
    },

    "input_size": [332, 32],
    "output": "output",
    "seed": 42,
    "device": "cuda"
}