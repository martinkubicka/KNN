{
    "data_dir": "../data/database/",
    "indicies": {
        "train": "../data/train_ids.csv",
        "val": "../data/val_ids.csv",
        "test": "../data/test_ids.csv"
    },

    "batch_size": 768,
    "epochs": 100,

    "scheduler": {
        "step_size": 10,
        "gamma": 0.1
    },

    "lr": 0.001,
    "momentum": 0.9,
    "weight_decay": 0.0001,

    "embedding": {
        "dim": 256,
        "dropout": 0.5
    },

    "architecture":{
        "name": "vit",
        "patch_size": 16,
        "num_classes": 4092  
    },

    "loss": {
        "name": "adaface"
    },

    "input_size": [384, 32],
    "output": "output",
    "seed": 42,
    "device": "cuda"

}